{
	"manifest_version": 2,

	"name": "LMS Hack",
	"description": "No LMS will stand against us!",
	"version": "0.1",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"activeTab",
		"webRequest",
		"webRequestBlocking"
	]
}